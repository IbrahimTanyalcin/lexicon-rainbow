{
    "extends": "eslint:recommended",
	 "env": {
        "browser": true,
        "node": true
    },
	  "globals": {
        "d3": true
    },
	  "rules": {
		"no-console": "off",
		"no-inner-declarations": "off",
        "no-redeclare": "off",	
		"no-unused-vars": "off",
		"no-fallthrough": "off",
		"no-empty": "off",
		"no-undef": "off"
    }
}